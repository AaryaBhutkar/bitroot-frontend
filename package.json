    },
    "scripts": {
        "start": "react-scripts start",
        "build": "GENERATE_SOURCEMAP=false eslint --ignore-path .gitignore . && react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "rules": {
            "react-hooks/exhaustive-deps": "off",
            "eqeqeq": "off",
            "no-console": "off",
            "no-unused-vars": "off"
        },
        "ignorePatterns": [
            "build/",
            "node_modules/"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "tailwindcss": "^3.4.4"
    }
}
